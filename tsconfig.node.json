{
  "compilerOptions": {
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "module": "esnext",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "noEmit": true
  },
  "include": ["vite.config.ts","src/**/*.ts","src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue","config/**/*.ts","env/*"],
}
